// The basic structure to a p5js code

//the variables
var a = 0;

//setup only runs once - sets the stage for the entire code
function setup() {
	createCanvas(500,500);
  background(0);
  
}

//draw is constantly running within the sketch
function draw() {
  rect(20,20,100,100);
  fill(255);
  stroke(240,100,150);
  strokeWeight(5);
}
