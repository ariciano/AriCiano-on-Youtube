function setup() { 
createCanvas(400, 400);
} 

function draw() { 
}

function keyPressed() {
 if (keyCode == UP_ARROW) {
   background (50, 100, 200);
 } else if (keyCode == DOWN_ARROW) {
   background (100,40,200);
 } else if (keyCode == 65) {
   background (24,255,171);
 } else if (keyCode == 83) {
   background (255, 83,26);
 } else if (keyCode == 68) {
   background (217, 162,0);
 } else if (keyCode == 90) {
   rect( 180,180,40,40);
   fill(0);
 } 
  return false;
}
